<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>

    <key>Label</key>
    <string>alfred-daily-cron</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>StartInterval</key>
    <integer>86400</integer> <!-- //every 24 hours -->
    
    <key>ProgramArguments</key>
    <array>
      <string>/usr/bin/osascript</string> <!-- use osascript to launch the script -->
      <string>/Users/rohit.bhosle/Library/Application Support/Alfred/Alfred.alfredpreferences/workflows/daily-cron-handler-workflow/script.scpt</string> <!-- the argument -->
    </array>

  </dict>
</plist>